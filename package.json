{
  "name": "realcube",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "ios:pods": "npx pod-install",
    "start": "react-native start",
    "web": "next dev",
    "build": "next build",
    "next-start": "next start",
    "test:all": "yarn test:jest",
    "test:jest": "jest",
    "lint:all": "yarn lint:eslint && yarn lint:prettier && yarn lint:types",
    "lint:eslint": "eslint src/",
    "lint:eslint:fix": "eslint --fix src/",
    "lint:prettier": "prettier --check \"src/**/*.+(js|jsx|ts|tsx|json|css|md)\"",
    "lint:prettier:fix": "prettier --write \"src/**/*.+(js|jsx|ts|tsx|json|css|md)\"",
    "lint:types": "tsc",
    "package:install": "yarn && yarn ios:pods || true",
    "package:update": "yarn upgrade --latest && cd ios && rm -f Podfile.lock && yarn ios:pods && pod update",
    "next-lint": "next lint",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@manaflair/redux-batch": "^1.0.0",
    "@react-native-async-storage/async-storage": "^1.17.11",
    "@react-native-masked-view/masked-view": "^0.2.8",
    "@react-navigation/drawer": "^6.5.8",
    "@react-navigation/native": "^6.1.3",
    "@reduxjs/toolkit": "^1.9.3",
    "@tamagui/animations-react-native": "^1.5.19",
    "@tamagui/animations-reanimated": "^1.5.19",
    "@tamagui/babel-plugin": "^1.5.19",
    "@tamagui/font-inter": "^1.5.19",
    "@tamagui/font-silkscreen": "^1.5.19",
    "@tamagui/lucide-icons": "^1.5.19",
    "@tamagui/next-plugin": "^1.5.19",
    "@tamagui/next-theme": "^1.5.19",
    "@tamagui/shorthands": "^1.5.19",
    "@tamagui/themes": "^1.5.19",
    "@types/node": "^18.13.0",
    "axios": "^1.3.4",
    "date-fns": "^2.29.3",
    "expo-file-system": "^15.2.2",
    "i18next": "^22.4.11",
    "next": "13.1.6",
    "raf": "^3.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.2.0",
    "react-icons": "^4.7.1",
    "react-intl": "^6.2.10",
    "react-native": "^0.71.3",
    "react-native-fast-image": "^8.6.3",
    "react-native-gesture-handler": "^2.9.0",
    "react-native-pager-view": "^6.1.2",
    "react-native-reanimated": "^2.14.4",
    "react-native-reanimated-swc-plugin": "^0.3.0",
    "react-native-safe-area-context": "^4.5.0",
    "react-native-screens": "^3.19.0",
    "react-native-svg": "^13.8.0",
    "react-native-tab-view": "^3.3.4",
    "react-native-web": "^0.18.12",
    "react-redux": "^8.0.5",
    "redux": "^4.2.1",
    "redux-batch": "^0.0.0",
    "redux-devtools-extension": "^2.13.9",
    "redux-persist": "^6.0.0",
    "redux-persist-expo-filesystem": "^2.0.0",
    "redux-saga": "^1.2.2",
    "sass": "^1.59.2",
    "sharp": "^0.31.3",
    "solito": "^2.3.0",
    "tamagui": "^1.5.19"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.10",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "29.2.1",
    "babel-plugin-transform-inline-environment-variables": "^0.4.4",
    "eslint": "^8.19.0",
    "eslint-config-next": "^13.1.2",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "^0.73.7",
    "patch-package": "^6.5.1",
    "pod-install": "^0.1.38",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.4.1",
    "react-native-dotenv": "^3.4.8",
    "react-test-renderer": "^18.2.0",
    "typescript": "^4.8.4"
  },
  "resolutions": {
    "@babel/plugin-transform-typescript": "7.19.3",
    "metro-react-native-babel-transformer": "0.73.7"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "transform": {
      "\\.[jt]sx?$": [
        "babel-jest",
        {
          "configFile": "./babel.config.native.js"
        }
      ]
    },
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg|solito)"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
